<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.docs.infos.model.impl.DistrictModelImpl">
		<attributes>
			<id name="districtId" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="name" />
			<basic name="code">
				<column name="CODE_" />
			</basic>
			<basic name="isArchived" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.docs.infos.model.impl.FarmerModelImpl">
		<attributes>
			<id name="farmerId" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="orgName" />
			<basic name="legalForm" />
			<basic name="inn" />
			<basic name="kpp" />
			<basic name="ogrn" />
			<basic name="regDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="isArchived" />
			<basic name="districtId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.docs.infos.model.impl.DistrictImpl" name="District">
		<table name="infos_District" />
		<attributes />
	</entity>
	<entity class="com.liferay.docs.infos.model.impl.FarmerImpl" name="Farmer">
		<table name="infos_Farmer" />
		<attributes />
	</entity>
</entity-mappings>